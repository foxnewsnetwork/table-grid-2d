<thead>
  <tr>
    <th class="origin">{{originLabel}}</th>
    {{#each colName in colNames}}
      <th>{{colName}}</th>
    {{/each}}
  </tr>
</thead>
<tbody>
  {{#each row in rows}}
    <tr>
      <td class="row-name">{{row.rowName}}</td>
      {{#each cell in row.cells}}
        <td {{action "touch" cell}}>{{yield cell}}</td>
      {{/each}}
    </tr>
  {{/each}}
</tbody>
<div hidden>{{rowsPromise}}</div>